function calcularRM (pesoLevantado , repeticiones){
    return pesoLevantado * (1 + repeticiones / 30);
}   

let pesoLevantado = parseFloat(prompt("¿Cuánto peso levantaste? (en kg)"));     
let repeticiones = parseInt(prompt("¿Cuántas repeticiones hiciste? (número entero)"));
let rm = calcularRM(pesoLevantado, repeticiones);
console.log(`Tu RM es: ${rm} kg`); // Muestra el resultado en la consola
console.log("¡Felicidades! Has calculado tu RM.");  